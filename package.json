{
  "name": "git-viewer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init-project": "npm i && npm run selenium-install && npm run pkg-upd",
    "init-all": "npm run git-upd && npm run init-project",
    "upd": "run-s git-upd pkg-upd",
    "git-upd": "git submodule update --init --recursive --remote",
    "pkg-upd": "cd html-reporter && npm i && cd ..",
    "start": "node ./bin/www",
    "test": "run-s unit-test intergraion-test",
    "selenium-install": "selenium-standalone install",
    "selenium": "selenium-standalone start",
    "gui": "hermione gui",
    "hemione": "hermione",
    "hermione-gui": "run-p test-server gui",
    "hermione-static": "run-p test-server hermione",
    "integration-test": "run-p test-server hermione",
    "unit-test": "mocha **/*.test.js",
    "test-server": "cd env-for-test && node ./www",
    "devstart": "ndb ./bin/www",
    "selenium-standalone": "^6.15.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "hbs": "^4.0.1",
    "html-reporter": "file:html-reporter",
    "npm-run-all": "^4.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "hermione": "^0.90.1",
    "mocha": "^5.2.0",
    "ndb": "^1.0.26",
    "selenium-standalone": "^6.15.3"
  }
}
